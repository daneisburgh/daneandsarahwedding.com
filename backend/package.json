{
    "name": "daneandsarahwedding-backend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serverless": "serverless",
        "wait-on": "wait-on",
        "migrate": "sequelize-cli db:migrate",
        "offline": "serverless offline --verbose",
        "build": "tsc --project tsconfig.json",
        "watch": "tsc --watch --preserveWatchOutput --project tsconfig.json",
        "serve": "npm run wait-on tcp:localhost:5432 && npm-run-all build migrate && npm-run-all --parallel offline watch",
        "deploy": "build && serverless create_domain && serverless deploy --verbose",
        "destroy": "serverless delete_domain && serverless remove --verbose"
    },
    "dependencies": {
        "lodash": "^4.17.15",
        "pg": "^8.0.0",
        "pg-hstore": "^2.3.3",
        "sequelize": "^5.21.5",
        "sequelize-cli": "^5.5.1",
        "serverless-aws-static-file-handler": "^2.0.5",
        "validator": "^12.2.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.30",
        "@types/lodash": "^4.14.149",
        "@types/node": "^8.10.59",
        "@types/validator": "^12.0.1",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "npm-run-all": "^4.1.5",
        "serverless": "^1.67.0",
        "serverless-domain-manager": "^3.3.1",
        "serverless-offline": "^5.12.1",
        "typescript": "^3.8.3",
        "wait-on": "^4.0.1"
    }
}
